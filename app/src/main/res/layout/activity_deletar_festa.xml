<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".eventos.gui.DeletarFestaActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="239dp"
        android:layout_height="55dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="71dp"
        android:fontFamily="cursive"
        android:text="Deletar Eventos"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/eventoDeletar"
        android:layout_width="309dp"
        android:layout_height="45dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="51dp"
        android:ems="10"
        android:hint="Nome do evento"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/deletarEventobutton"
        android:layout_width="177dp"
        android:layout_height="45dp"
        android:layout_marginStart="117dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="117dp"
        android:background="@drawable/buttonroundpurple"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="Deletar Evento"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eventoDeletar" />
</android.support.constraint.ConstraintLayout>